{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:vue/essential"
  ],
  // "extends": [
  //   "eslint:recommended",
  //   "plugin:prettier/recommended",
  //   "plugin:vue/vue3-recommended"
  // ],
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "vue/v-bind-style": ["error", "shorthand"],
    "curly": "error"
  }
}
